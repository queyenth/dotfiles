#include "/home/queyenth/.colors/queyenth"

urxvt*perl-lib: /usr/lib/urxvt/perl
urxvt*perl-ext-common: default,matcher,font-size
urxvt*perl-matcher: 1
urxvt*matcher-button: 3
urxvt*url-launcher: /usr/bin/firefox
urxvt*depth: 32
urxvt*dynamicColors: on
urxvt*scrollBar: false
urxvt*geometry: 80x20
urxvt*cursorBlink: false
urxvt*cursorUnderline: false
urxvt*internalBorder: 5
urxvt.urgentOnBell: true

urxvt*font: xft:Monaco for Powerline:style=Medium:size=8:dpi=96,-misc-stlarch-medium-r-normal--10-100-75-75-c-80-iso10646-1
urxvt*boldFont: xft:Monaco for Powerline:style=Bold:size=8:dpi=96,-misc-stlarch-medium-r-normal--10-100-75-75-c-80-iso10646-1
urxvt*letterSpace: -1

URxvt.iso14755: false
URxvt.iso14755_52: false

Xft.dpi: 96
Xft.antialias: 1
Xft.autohint: 0
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
