#include "/home/queyenth/.colors/queyenth"
# fallac, fresh, hybrid, jmbi, smooth, venam

urxvt*perl-lib: /usr/lib/urxvt/perl
urxvt*perl-ext-common: default,matcher,clipboard,font-size
urxvt*perl-matcher: 1
urxvt*matcher-button: 3
urxvt*url-launcher: /usr/bin/firefox
urxvt*depth: 32
urxvt*dynamicColors: on
urxvt*scrollBar: false
urxvt*geometry: 95x15
urxvt*cursorBlink: false
urxvt*cursorUnderline: false
urxvt*internalBorder: 3
urxvt.urgentOnBell: true

urxvt*font: xft:ubuntu mono derivative powerline:size=9:dpi=96
urxvt*boldFont: xft:ubuntu mono derivative powerline:style=Bold:size=9:dpi=96
urxvt*letterSpace: -1

/* urxvt*font: -misc-tamzen-medium-r-normal--11-0-100-100-c-0-iso8859-1 */
/* urxvt*boldFont: -misc-tamzen-medium-r-normal--11-0-100-100-c-0-iso8859-1 */
/* urxvt*letterSpace: 1 */

Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
